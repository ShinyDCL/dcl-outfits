{
  "name": "dcl-outfits",
  "version": "1.0.0",
  "description": "Decentraland scene displaying users outfits",
  "scripts": {
    "start": "sdk-commands start",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next",
    "upgrade-dcl-deps": "npm install --save-dev @dcl/sdk@latest @dcl/js-runtime@latest && npm i @dcl-sdk/utils"
  },
  "devDependencies": {
    "@dcl/js-runtime": "7.3.35",
    "@dcl/sdk": "^7.3.35"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "@dcl-sdk/utils": "^1.2.1"
  }
}